.zt_newslist .clicktoday h3 a:hover{ color:#f44d4d;}
.zt_newslist .clicktoday ul li{ width:100%; overflow:hidden; }
.zt_newslist .clicktoday ul li a{ width:200px;float:left;}
.zt_newslist .clicktoday ul li i, .zt_newslist .clicktoday h3 i{ font-style:normal; font-weight: normal; color:#999; float:right; font-size:12px; line-height:24px;}

.zt_more{ width:920px; overflow:hidden; margin:15px auto;}
.zt_more h2{ widows:100%; color:#f44d4d; font-size:16px; }
.zt_more_item{ border:1px solid #ebebeb; padding:5px; margin:10px auto; overflow:hidden;}
.zt_more_item a{ display:block; float:left; width:150px; text-align:center; border-right:1px solid #ebebeb; padding:15px 0; }
.zt_more_item a:last-child{ border-right:none;}
.zt_more_item a i{ width:30px; height:30px; display:inline-block; margin-right:8px; background-image:url(../images/zt_icon.png); background-repeat:no-repeat; vertical-align:middle;}
.zt_more_item a span{ display:inline-block;  line-height:30px; font-weight:bold; font-size:14px; color:#666; vertical-align:middle;}
.zt_more_item a i.zt_icon1{ background-position:-6px -8px;}
.zt_more_item a i.zt_icon2{ background-position:-6px -57px;}
.zt_more_item a i.zt_icon3{ background-position:-6px -109px;}
.zt_more_item a i.zt_icon4{ background-position:-6px -161px;}
.zt_more_item a i.zt_icon5{ background-position:-6px -212px;}
.zt_more_item a i.zt_icon6{ background-position:-6px -262px;}
.zt_more_item a:hover i.zt_icon1{ background-position:-66px -8px;}
.zt_more_item a:hover i.zt_icon2{ background-position:-66px -57px;}
.zt_more_item a:hover i.zt_icon3{ background-position:-66px -109px;}
.zt_more_item a:hover i.zt_icon4{ background-position:-66px -161px;}
.zt_more_item a:hover i.zt_icon5{ background-position:-66px -212px;}
.zt_more_item a:hover i.zt_icon6{ background-position:-66px -262px;}
.zt_more_item a:hover span{ color:#f44d4d;}

/*新闻内容列表页*/
.newlistc{width:100%; height:auto; overflow:hidden; padding-bottom:50px;}
.newlistc ul{width:100%; height:auto; overflow:hidden; padding:12px 0; border-bottom:#ccc dashed 1px;}
.newlistc ul li{width:100%; height:30px; overflow:hidden; float:left;}
.listent:link,.listent:visited{text-decoration:none; color:#4f5157; float:left; line-height:30px; background:url(../images/newlist_icon.gif) no-repeat 2px 12px; padding:0 0 0 15px; font-size:14px;}
.listent:hover{text-decoration:none; color:#44BBFF;}
.newlistc ul li span{float:right; line-height:30px; color:#999999;}

/*--上一篇下一篇--*/
.article{width:100%; height:auto; overflow:hidden; padding:10px 0;}
.article ul li{width:100%; height:auto; overflow:hidden; float:left; line-height:26px; color:#000;}
.article ul li a:link,.article ul li a:visited{text-decoration:none; color:#333333;}
.article ul li a:hover{text-decoration:underline; color:#FF0000;}

.picbtn{width:640px; height:auto; overflow:hidden; position:relative;}
.picbtn_l{width:320px; min-height:550px; overflow:hidden; position:absolute; left:0; top:0; display:block; z-index:666; }
.picbtn_r{width:320px; min-height:550px; overflow:hidden; position:absolute; top:0; display:block; right:0; z-index:666; }
.picbtn_l:hover{background:url(../images/btn_l.png) no-repeat 80px 240px;}
.picbtn_r:hover{background:url(../images/btn_r.png) no-repeat 208px 240px;}















